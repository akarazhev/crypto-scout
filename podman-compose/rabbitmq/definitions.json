{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password_hash": "",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "parameters": [],
  "policies": [
    {
      "vhost": "/",
      "name": "ha-policy",
      "pattern": "^(metrics-.*|crypto-.*)",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic"
      },
      "priority": 1
    }
  ],
  "queues": [
    {
      "name": "metrics-cmc-fear-greed-index-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 2500,
        "x-overflow": "reject-publish",
        "x-message-ttl": 21600000,
        "x-dead-letter-exchange": "",
        "x-dead-letter-routing-key": "metrics-dead-letter-queue"
      }
    },
    {
      "name": "metrics-bybit-launch-pool-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 2500,
        "x-overflow": "reject-publish",
        "x-message-ttl": 21600000,
        "x-dead-letter-exchange": "",
        "x-dead-letter-routing-key": "metrics-dead-letter-queue"
      }
    },
    {
      "name": "metrics-dead-letter-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "crypto-scout-client-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 2500,
        "x-overflow": "reject-publish",
        "x-message-ttl": 21600000
      }
    },
    {
      "name": "service.notifications",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 100000,
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "service.commands",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 50000,
        "x-overflow": "reject-publish"
      }
    }
  ],
  "exchanges": [
    {
      "name": "metrics-exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "crypto.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "service.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "metrics-exchange",
      "vhost": "/",
      "destination": "metrics-cmc-fear-greed-index-queue",
      "destination_type": "queue",
      "routing_key": "metrics.cmc_fear_greed_index",
      "arguments": {}
    },
    {
      "source": "metrics-exchange",
      "vhost": "/",
      "destination": "metrics-bybit-launch-pool-queue",
      "destination_type": "queue",
      "routing_key": "metrics.bybit_launch_pool",
      "arguments": {}
    },
    {
      "source": "crypto.events",
      "vhost": "/",
      "destination": "service.notifications",
      "destination_type": "queue",
      "routing_key": "crypto.price.#",
      "arguments": {}
    },
    {
      "source": "service.events",
      "vhost": "/",
      "destination": "service.commands",
      "destination_type": "queue",
      "routing_key": "service.command.#",
      "arguments": {}
    },
    {
      "source": "service.events",
      "vhost": "/",
      "destination": "crypto-scout-client-queue",
      "destination_type": "queue",
      "routing_key": "commands",
      "arguments": {}
    }
  ],
  "stream_connections": [],
  "streams": [
    {
      "name": "crypto.price.stream",
      "vhost": "/",
      "arguments": {
        "max-age": "7D",
        "max-segment-size-bytes": 100000000,
        "max-length-bytes": 20000000000,
        "queue-leader-locator": "balanced"
      }
    },
    {
      "name": "crypto.market.stream",
      "vhost": "/",
      "arguments": {
        "max-age": "7D",
        "max-segment-size-bytes": 100000000,
        "max-length-bytes": 20000000000,
        "queue-leader-locator": "balanced"
      }
    }
  ]
}
