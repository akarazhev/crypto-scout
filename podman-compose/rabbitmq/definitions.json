{
  "rabbit_version": "4.1.3",
  "rabbitmq_version": "4.1.3",
  "product_name": "RabbitMQ",
  "product_version": "4.1.3",
  "users": [
    {
      "name": "admin",
      "password_hash": "",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "parameters": [],
  "policies": [
    {
      "vhost": "/",
      "name": "ha-policy",
      "pattern": "^(metrics-.*|crypto-.*)",
      "apply-to": "all",
      "definition": {
        "queue-leader-locator": "balanced",
        "queue-mode": "lazy"
      },
      "priority": 1
    },
    {
      "vhost": "/",
      "name": "stream-retention-policy",
      "pattern": "^crypto\\.(price|market)\\.stream$",
      "apply-to": "streams",
      "definition": {
        "max-age": "7D"
      },
      "priority": 1
    }
  ],
  "queues": [
    {
      "name": "metrics-cmc-fgi-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 2500,
        "x-overflow": "reject-publish",
        "x-message-ttl": 21600000,
        "x-dead-letter-exchange": "",
        "x-dead-letter-routing-key": "metrics-dead-letter-queue"
      }
    },
    {
      "name": "metrics-bybit-lpl-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 2500,
        "x-overflow": "reject-publish",
        "x-message-ttl": 21600000,
        "x-dead-letter-exchange": "",
        "x-dead-letter-routing-key": "metrics-dead-letter-queue"
      }
    },
    {
      "name": "metrics-dead-letter-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "crypto-scout-client-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 2500,
        "x-overflow": "reject-publish",
        "x-message-ttl": 21600000
      }
    },
    {
      "name": "service.notifications",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 100000,
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "service.commands",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 50000,
        "x-overflow": "reject-publish"
      }
    }
  ],
  "exchanges": [
    {
      "name": "metrics-exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "crypto.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "service.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "metrics-exchange",
      "vhost": "/",
      "destination": "metrics-cmc-fgi-queue",
      "destination_type": "queue",
      "routing_key": "metrics.cmc_fgi",
      "arguments": {}
    },
    {
      "source": "metrics-exchange",
      "vhost": "/",
      "destination": "metrics-bybit-lpl-queue",
      "destination_type": "queue",
      "routing_key": "metrics.bybit_lpl",
      "arguments": {}
    },
    {
      "source": "crypto.events",
      "vhost": "/",
      "destination": "service.notifications",
      "destination_type": "queue",
      "routing_key": "crypto.price.#",
      "arguments": {}
    },
    {
      "source": "service.events",
      "vhost": "/",
      "destination": "service.commands",
      "destination_type": "queue",
      "routing_key": "service.command.#",
      "arguments": {}
    },
    {
      "source": "service.events",
      "vhost": "/",
      "destination": "crypto-scout-client-queue",
      "destination_type": "queue",
      "routing_key": "commands",
      "arguments": {}
    }
  ],
  "stream_connections": [],
  "streams": [
    {
      "name": "crypto.price.stream",
      "vhost": "/",
      "arguments": {
        "max-age": "7D",
        "queue-leader-locator": "balanced"
      }
    },
    {
      "name": "crypto.market.stream",
      "vhost": "/",
      "arguments": {
        "max-age": "7D",
        "queue-leader-locator": "balanced"
      }
    }
  ],
  "feature_flags": [
    {
      "name": "stream_queue",
      "enabled": true
    },
    {
      "name": "direct_exchange_routing_v2",
      "enabled": true
    },
    {
      "name": "maintenance_mode_status",
      "enabled": true
    },
    {
      "name": "user_limits",
      "enabled": true
    },
    {
      "name": "virtual_host_metadata",
      "enabled": true
    }
  ]
}
