{
  "streams": [
    {
      "name": "crypto-bybit-stream",
      "vhost": "/",
      "arguments": {
        "max-age": "7D",
        "queue-leader-locator": "balanced"
      }
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "stream-retention-policy",
      "pattern": "^crypto\\.(bybit)\\.stream$",
      "apply-to": "streams",
      "definition": {
        "max-age": "7D"
      },
      "priority": 1
    }
  ]
}